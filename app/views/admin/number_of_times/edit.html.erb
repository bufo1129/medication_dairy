<div class="text-center mb-5"></div>

<div class="container">
  <div class="row">
    <%= form_with model: @number_of_time, url: admin_number_of_time_path, method: :patch do |f| %>
    <h3 class="font-weight-bold col-3 ml-4 mb-4 text-nowrap"><strong>回数登録の編集</strong></h3>
    <div class="form-group">
      <label for="times_name">登録名:</label>
      <%= f.text_field :name, class: "form-control" %>
    </div>
    <div class="form-group">
      <label for="dosing_times">回数:</label>
      <%= f.text_field :dosing_times, class: "form-control" %>
    </div>
      <%= f.submit "変更する", class: "btn btn-success" %>
    <% end %>
  </div>
</div>
